#application port configuration
server.port=8181

debug=true
logging.level.com.zjhc.sgsb.mapper=DEBUG

spring.jackson.date-format=yyyy/MM/dd HH:mm:ss
spring.jackson.time-zone=GMT+8

#dataSource configuration
spring.datasource.name=bk
spring.datasource.url=jdbc\:mysql\://localhost\:3306/sgsb?useUnicode\=true&characterEncoding\=utf8&zeroDateTimeBehavior\=convertToNull&autoReconnect\=true&allowMultiQueries\=true&serverTimezone=GMT%2B8
spring.datasource.username=root
spring.datasource.password=115820
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.type=com.alibaba.druid.pool.DruidDataSource

mybatis-plus.mapper-locations= classpath*:mapper/*Mapper.xml

spring.resources.static-locations=classpath:/META-INF/resources/,classpath:/resources/,classpath:/static/,classpath:/public/,file:/root/static/

logging.file=log//server.log

